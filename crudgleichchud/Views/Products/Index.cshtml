@{
}
@model List<Product>
<h1> Produkt-Tabelle</h1>

<table class="table">
    <tr>
        
        <th>ID</th>
        <th>Name</th>
        <th>Price</th>
            
    </tr>
    @foreach (Product product in Model)
    {
        <tr>
            <td>@product.Id</td>
            <td>@product.Name</td>
            <td>@product.Price</td>
        </tr>
    }
</table>
